{
  "data": {
    "phishing_urls": 10000,
    "legitimate_urls": 10000,
    "test_size": 0.2,
    "validation_size": 0.1
  },
  "features": {
    "url_features": true,
    "domain_features": true,
    "content_features": true,
    "behavioral_features": false
  },
  "models": {
    "lightgbm": {
      "enabled": true,
      "params": {
        "objective": "binary",
        "metric": "binary_logloss",
        "boosting_type": "gbdt",
        "num_leaves": 31,
        "learning_rate": 0.05,
        "feature_fraction": 0.9,
        "bagging_fraction": 0.8,
        "bagging_freq": 5,
        "verbose": -1
      }
    },
    "tensorflow": {
      "enabled": true,
      "architecture": "deep",
      "epochs": 50,
      "batch_size": 256,
      "learning_rate": 0.001
    },
    "xgboost": {
      "enabled": true,
      "params": {
        "objective": "binary:logistic",
        "eval_metric": "logloss",
        "max_depth": 6,
        "learning_rate": 0.1,
        "subsample": 0.8,
        "colsample_bytree": 0.8
      }
    }
  },
  "optimization": {
    "hyperparameter_tuning": false,
    "cross_validation_folds": 5,
    "early_stopping": true
  },
  "output": {
    "save_models": true,
    "save_predictions": true,
    "generate_report": true
  }
}
